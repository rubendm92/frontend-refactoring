{
  "name": "frontend-refactoring",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "$(npm bin)/webpack",
    "start": "npm run build && $(npm bin)/serve -p 8000",
    "test_unit": "$(npm bin)/mocha --require babel-register tests/unit",
    "test_integration": "npm run build && $(npm bin)/mocha -r babel-polyfill -r babel-register -r jsdom-global/register tests/integration",
    "test_e2e": "npm run build && $(npm bin)/mocha tests/e2e",
    "test": "npm run test_unit && npm run test_integration && npm run test_e2e"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.5",
    "puppeteer": "^1.2.0",
    "serve": "^6.5.3",
    "sinon": "^4.5.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
